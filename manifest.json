{
  "manifest_version": 3,
  "name": "Straight Up Zillowing It",
  "version": "1.0",
  "description": "Adds a Zillow button next to US addresses on any webpage.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    },
    {
      "matches": ["https://*.realtor.com/*"],
      "js": ["realtor-prefill.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*.har.com/*"],
      "js": ["har-prefill.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "suzi.png"
  }
}
